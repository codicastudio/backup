{
    "name": "codica-studio/backup",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codica-studio/backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/notifications": "^8.0",
        "illuminate/support": "^8.0",
        "league/flysystem": "^1.0.49",
        "spatie/db-dumper": "^2.12",
        "spatie/temporary-directory": "^1.1",
        "symfony/finder": "^4.2|^5.0"
    },
    "require-dev": {
        "laravel/slack-notification-channel": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Backup\\": "src"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Backup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=no"
    },
    "suggest": {
        "laravel/slack-notification-channel": "Required for sending notifications via Slack"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Backup\\BackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
